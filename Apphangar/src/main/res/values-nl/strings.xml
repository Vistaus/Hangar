    <!-- Donate -->
    <string name="donate_title">Wil je me een biertje schenken?</string>
    <string name="donate_head">Donaties zijn altijd welkom helpen om deze en andere apps te kunnen blijven ontwikkelen.\n\nMaar als je Hangar fijn vindt en me wilt bedanken, ga dan naar de sectie Bijdragen om te zien hoe je nog meer kunt helpen.\n\nAls je in een gulle bui bent, dan kun je hieronder doneren via <b>Google Play</b>.</string>
    <string name="donate_title_google" translatable="false">Google Play</string>
    <string name="donate_google">Doneren met Google</string>
    <string name="donate_contribute">Help Hangar te verbeteren!</string>
    <string name="donate_accept_button">"Ga zo door!"</string>
    <string-array name="entries_google_donate" translatable="false">
        <item>$1 (USD)</item>
        <item>$2 (USD)</item>
        <item>$3 (USD)</item>
        <item>$4 (USD)</item>
        <item>$5 (USD)</item>
    </string-array>
